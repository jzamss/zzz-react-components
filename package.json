{
  "name": "zzz-react-components",
  "version": "0.1.1",
  "description": "ZZZ React Components",
  "author": "zzz",
  "license": "MIT",
  "repository": "zzz/zzz-react-components",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --sourcemap false --compress --css-modules true --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --css-modules true  --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.20.0",
    "date-fns": "^2.15.0",
    "dayjs": "^1.9.6",
    "final-form": "4.20.1",
    "final-form-focus": "^1.1.2",
    "format-string-by-pattern": "1.2.2",
    "mui-rff": "3.0.3",
    "react-final-form": "6.5.2",
    "react-final-form-listeners": "^1.0.3",
    "react-modal": "^3.11.2",
    "react-number-format": "4.4.4",
    "react-text-mask": "^5.4.3",
    "socket.io-client": "^2.3.0"
  }
}
